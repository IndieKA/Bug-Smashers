var draw = function() {
    background(161, 161, 161);
    textAlign(CENTER, CENTER);
    fill(255, 255, 255);
    scale(1.5,1.5);
    sprite1();
    sprite2();
    sprite3();
    sprite4();
    sprite5();
    sprite6();
    sprite7();
    sprite8();

    enemy1();
    enemy2();
    enemy3();
    enemy4();
    enemy5();
    enemy6();
    enemy7();
    enemy8();
};
